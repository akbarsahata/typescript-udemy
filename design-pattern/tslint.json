{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "indent": [true, "spaces", 2],
        "space-before-function-paren": [true, "always"],
        "max-classes-per-file": false,
        "quotemark": [true, "single", "avoid-escape"]
    },
    "rulesDirectory": []
}