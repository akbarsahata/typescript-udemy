{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "semicolon": [
      true,
      "never"
    ],
    "space-before-function-paren": [
      true,
      "always"
    ],
    "trailing-comma": false,
    "max-line-length": [
      true,
      {
        "limit": 80,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}